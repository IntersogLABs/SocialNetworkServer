<h1>Edit profile</h1>
<p>Fill the fields you want to change</p>
<form role="form">
  <div>
    <input type="email" name="email" placeholder="E-mail"/>
  </div>
  <div>
    <input name="nick" placeholder="Nick"/>
  </div>
  <div>
    <input type="password" name="pwd" placeholder="Password"/>
  </div>
  <button class="btn btn-default mydata-submit">
    Update
  </button>
</form>
